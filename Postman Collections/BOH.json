		{
			"name": "Inventory dashboard",
			"item": [
				{
					"name": "Get Inventory Summary For Dashboard",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "d02f7d6f-cb1b-4163-9f86-26450d33459a",
								"exec": [
									"pm.test(\"Response status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Response is an object with title, data, statusCode, and isSuccess properties\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.be.an('object').that.has.all.keys('title', 'data', 'statusCode', 'isSuccess');\r",
									"});\r",
									"\r",
									"pm.test(\"Each itemized inventory has properties name, beginningPerpetual, totalIn, totalOut, currentPerpetual, and id\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    jsonData.data.itemizedInventories.forEach(function(item) {\r",
									"        pm.expect(item).to.be.an('object').that.has.all.keys('name', 'beginningPerpetual', 'totalIn', 'totalOut', 'currentPerpetual', 'id');\r",
									"    });\r",
									"});\r",
									"\r",
									"pm.test(\"Each category summary has properties name and count\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    jsonData.data.categorySummaries.forEach(function(category) {\r",
									"        pm.expect(category).to.be.an('object').that.has.all.keys('name', 'count');\r",
									"    });\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
